@model Student

@{
    ViewData["Title"] = "Details";
}

<h2>Details</h2>
<hr />


<dl class="dl-horizontal">
    <dt><label asp-for="FullName"></label></dt>
    <dd>Model.FullName</dd>

    <dt><label asp-for="BirthDate"></label></dt>
    <dd>@Model.BirthDate</dd>

    <dt><label asp-for="Pesel"></label></dt>
    <dd>@Model.Pesel</dd>

    <dt><label asp-for="GudrdianPhoneNumber"></label></dt>
    <dd>@Model.GudrdianPhoneNumber</dd>
</dl>
<div>
    <a asp-action="Edit" asp-route-id="@Model.ID">Edit</a> |
    <a asp-action="List">Back to List</a>
</div>
